{
  "name": "portfolio-vue",
  "version": "1.0.0",
  "description": "Waqas Ahmad Portfolio - Vue.js SPA",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/cleanup-dist.js",
    "build": "node scripts/generate-sw-version.js && vite build && node scripts/copy-404.js && node scripts/create-nojekyll.js && node scripts/create-headers.js && node scripts/copy-llms-txt.js && node scripts/generate-sitemap.js && node scripts/validate-sitemap.js && node scripts/generate-sw.js",
    "prebuild:firebase": "node scripts/cleanup-dist.js",
    "prebuild:firebase:dev": "node scripts/cleanup-dist.js",
    "build:firebase": "node scripts/generate-sw-version.js && node scripts/generate-favicon-ico.js && vite build --mode firebase && node scripts/copy-404.js && node scripts/create-nojekyll.js && node scripts/create-headers.js && node scripts/copy-llms-txt.js && node scripts/generate-sitemap.js && node scripts/write-robots-firebase.js && node scripts/validate-sitemap.js && node scripts/generate-sw.js",
    "build:firebase:dev": "node scripts/generate-sw-version.js && vite build --mode firebase-dev && node scripts/copy-404.js && node scripts/create-nojekyll.js && node scripts/create-headers.js && node scripts/copy-llms-txt.js && node scripts/generate-sitemap.js && node scripts/write-robots-firebase.js && node scripts/validate-sitemap.js && node scripts/generate-sw.js",
    "prerender": "node scripts/prerender-routes.js",
    "check-pages": "node scripts/check-pages-load.js",
    "check-pages:no-server": "node scripts/check-pages-load.js --no-start",
    "check-pages:dev": "node scripts/check-pages-load.js --no-start --port=5173 --base=/",
    "deploy:firebase": "node scripts/deploy-firebase.js",
    "deploy:firebase:dev": "node scripts/deploy-firebase.js --dev",
    "deploy:firebase:prod": "node scripts/deploy-firebase.js",
    "deploy:firebase:hosting": "firebase deploy --only hosting --project waqasahmad-portfolio",
    "preview": "vite preview",
    "serve": "vite preview",
    "optimize-service-images": "node scripts/optimize-service-images.js",
    "optimize-icons": "node scripts/optimize-icons.js",
    "optimize-gallery-images": "node scripts/optimize-gallery-images.js",
    "optimize-portfolio-thumbs": "node scripts/optimize-portfolio-thumbs.js",
    "optimize-critical-images": "node scripts/optimize-critical-images.js",
    "optimize-all-icons": "node scripts/optimize-all-icons.js",
    "convert-svgs-to-webp": "node scripts/convert-svgs-to-webp.js",
    "convert-svgs-to-png": "node scripts/convert-svgs-to-webp.js",
    "generate-icon-sizes": "node scripts/generate-icon-sizes.js",
    "generate-mobile-hero": "node scripts/generate-mobile-hero-image.js",
    "check-performance": "node scripts/check-performance.js",
    "check-performance:url": "node scripts/check-performance.js",
    "crawl-as-googlebot": "node scripts/crawl-as-googlebot.js",
    "analyze": "node scripts/analyze-seo.js",
    "analyze:crawl": "node scripts/crawl-as-googlebot.js --save-html=scripts/.cache/googlebot-crawl.html && node scripts/analyze-seo.js --html=scripts/.cache/googlebot-crawl.html",
    "submit-bing": "node scripts/submit-bing-indexnow.js",
    "verify-urls": "node scripts/verify-urls-indexing.js",
    "test:rankings": "node scripts/test-keyword-rankings.js",
    "test:rankings:20": "node scripts/test-keyword-rankings.js 20",
    "test:google-rankings": "powershell -ExecutionPolicy Bypass -File scripts/test-google-rankings.ps1",
    "test:google-rankings:20": "powershell -ExecutionPolicy Bypass -File scripts/test-google-rankings.ps1 20",
    "setup:serpapi": "powershell -ExecutionPolicy Bypass -File scripts/setup-serpapi.ps1",
    "auto-setup:serpapi": "powershell -ExecutionPolicy Bypass -File scripts/auto-setup-serpapi.ps1",
    "test:google-serpapi": "node scripts/test-google-rankings-serpapi.js",
    "test:google-serpapi:20": "node scripts/test-google-rankings-serpapi.js 20",
    "check:indexing": "node scripts/check-indexing-status.js",
    "submit:indexing": "powershell -ExecutionPolicy Bypass -File scripts/submit-google-indexing.ps1",
    "setup:google-cse": "powershell -ExecutionPolicy Bypass -File scripts/setup-google-cse-api.ps1",
    "setup:cse-complete": "powershell -ExecutionPolicy Bypass -File scripts/complete-setup.ps1",
    "setup:cse-finalize": "powershell -ExecutionPolicy Bypass -File scripts/finalize-setup.ps1",
    "setup:bing-api": "powershell -ExecutionPolicy Bypass -File scripts/setup-bing-api.ps1",
    "run-checks": "powershell -ExecutionPolicy Bypass -File scripts/run-checks.ps1"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@microsoft/clarity": "^1.0.2",
    "@panzoom/panzoom": "^4.6.0",
    "chart.js": "^4.5.0",
    "highlight.js": "^11.9.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-multimd-table": "^4.2.3",
    "mermaid": "^11.4.0",
    "swiper": "^12.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue3-carousel": "^0.16.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@unhead/vue": "^2.1.2",
    "@vitejs/plugin-vue": "^5.0.0",
    "png-to-ico": "^3.0.1",
    "puppeteer": "^24.36.0",
    "sass": "^1.93.2",
    "sharp": "^0.34.5",
    "vite": "^5.4.0",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-ssg": "^28.2.2"
  },
  "keywords": [
    "portfolio",
    "vue",
    "spa",
    "developer"
  ],
  "author": "Waqas Ahmad",
  "license": "MIT"
}
